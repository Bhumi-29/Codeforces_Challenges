#include<stdio.h>
#include<iostream>
#include<string>
#include<vector>
#include<algorithm>
#include<cstdlib>
#include<bits/stdc++.h>
using namespace std;

int main(){
     int T;
    cin>>T;
    int suma=0,sumb=0,sumc=0;
    for(int i=0;i<T;i++){
        int a,b,c;
        cin>>a>>b>>c;
        suma=suma+a;
        sumb=sumb+b;
        sumc=sumc+c;
    }
    if(suma==0&&sumb==0&&sumc==0){
        cout<<"YES"<<endl;
    }else{
        cout<<"NO"<<endl;
    }
}

