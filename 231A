#include<bits/stdc++.h>
#include <stdio.h>
using namespace std;
int main()
{
    int n;
    int count=0;
    cin >> n;
    for(int i=0;i<n;i++){
        int a,b,c;
        cin >> a >>b>>c;
        int sum=0;
        sum= sum+a+b+c;
        if(sum>1){
            count++;
        }
        
    }
    cout << count<<endl;
}
