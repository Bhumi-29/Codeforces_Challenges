#include<stdio.h>
#include<iostream>
#include<cstdlib>
#include<bits/stdc++.h>
using namespace std;
int main(){
 vector<int>v;
 for(int i=0;i<4;i++){
     int b;
     cin>>b;
     v.push_back(b);
 }
 sort(v.begin(),v.end());int count=0;
 for(int i=0;i<4;i++){
     if(v[i]==v[i+1]){
         count++;
     }
 }
 cout<<count;
}
